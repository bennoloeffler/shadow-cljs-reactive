;; shadow-cljs configuration
{:source-paths
 ["src"]

 :dependencies [[reagent "1.0.0"]]

 :builds
 
 {:app {:target :browser
        :output-dir "public/js"
        :asset-path "/js"

        :modules
        {:server ;; <- becomes public/js/server.js
         {:entries [starter.browser]}}

        ;; start a development http server on http://localhost:8020
        :devtools
        {:http-root "public"
         :http-port 8020}}}}
        
